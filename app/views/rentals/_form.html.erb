  <div class="container">
      <%= simple_form_for [art, rental] do |f| %>
      <h3>New Rental Form</h3>
      <h4>Get Your Custom Quote</h4>
        <%= f.input :address, class: "address" %>
        <%= f.input :rental_start, as: :string, required: false, input_html: {class: "datepicker"} %>
        <%= f.input :rental_end, as: :string, required: false, input_html: {class: "datepicker"} %>
        <%= f.button :submit, "Submit", class: 'rental_button' %>
      <% end %>
  </div>
</div>




