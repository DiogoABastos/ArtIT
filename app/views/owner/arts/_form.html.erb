<%= simple_form_for [owner, art] do |f| %>
  <%= f.input :name %>
  <%= f.input :artist %>
  <%= f.input :style %>
  <%= f.input :era %>
  <%= f.input :price %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :photo, as: :file %>
  <%= f.submit class: 'btn btn-success'%>
<% end %>
